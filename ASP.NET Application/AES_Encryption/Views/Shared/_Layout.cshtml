<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - AES Encryption</title>
    <link rel="icon" href="https://www.pymnts.com/wp-content/uploads/2016/08/security-customer-experience-banking.jpg"/>
    
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color: #563D7C;">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("AES Encryption", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="margin-left:45px;">@Html.ActionLink("Encryption", "Encryption", "Home")</li>
                    <li>@Html.ActionLink("Decryption", "Decryption", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
        <!--    <p>&copy; @DateTime.Now.Year - Grupi 1 - Siguria e informacionit</p> -->
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
